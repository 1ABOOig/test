
#define _CRT_SECURE_NO_WARNINGS 1

//复习c语言

#include "stdio.h"
#include "string.h"

int main()
{
	int a = 10;
	int* p = &a;
	printf("%p\n", &a);
	printf("%p\n", p);
	*p = 20;
	printf("a=%d\n", a);
	return 0;
}





